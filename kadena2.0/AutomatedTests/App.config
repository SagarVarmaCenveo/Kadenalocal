<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="envinment" value="Dev"/>
    
    <add key="Dev" value="http://default.kadenadev.com"/>
    <add key="Test" value="http://default.kadenatest.com"/>
    <add key="Stage" value="http://default.kadenastage.com"/>

    <add key="username" value="AutomationBotAdmin"/>
    <add key="password" value="bot2548"/>

    <add key="dateformat" value="yyyyMMdd" />
    <add key="datetimeformat" value="yyyyMMdd-HHmmss" />
    <add key="readabledateformat" value="dd. MM. yyyy - HH:mm:ss" />
    <!-- Path to directory of logs, {0} represents date in 'dateformat'-->
    <add key="logdirectory" value="\Log\"/>
    <!-- Path to log file, {0} represents current date in 'datetimeformat' format -->
    <add key="logname" value="testlog.{0}.txt"/>
    <!-- Path to directory of screenshots, {0} represents date in 'dateformat'-->
    <add key="screenshotdirectory" value="\Screenshots\" />
    <!-- Name of taken screenshot, {0} represents current date in 'datetimeformat' format and {1} represents name of test, has to contain suffix -->
    <add key="screenshotname" value="{1}.{0}.png" />
  </appSettings>
</configuration>