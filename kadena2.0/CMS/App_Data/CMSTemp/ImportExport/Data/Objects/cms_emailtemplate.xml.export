    <cms_emailtemplate>
      <EmailTemplateID>166</EmailTemplateID>
      <EmailTemplateName>Ecommerce.OrderNotificationToCustomer</EmailTemplateName>
      <EmailTemplateDisplayName>E-commerce - Order notification to customer</EmailTemplateDisplayName>
      <EmailTemplateText>&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body&gt;
&lt;p&gt;Thank you for your order. Below you can find the order details.&lt;/p&gt;
&lt;table width="600" cellspacing="0" cellpadding="5" border="1" style="border-color:black;"&gt;
    &lt;tbody&gt;
        &lt;tr&gt;
            &lt;td height="50" valign="bottom" colspan="2"&gt;
            &lt;table width="100%"&gt;
                &lt;tbody&gt;
                    &lt;tr&gt;
                        &lt;td style="text-align: left; vertical-align: bottom;"&gt;&lt;span style="font-size: 18pt;"&gt;Your order&lt;/span&gt;&lt;/td&gt;
                        &lt;td style="text-align: center; vertical-align: middle;"&gt;&lt;span style="font-family: Garamond,Times,serif; font-size: 24pt; font-style: italic;"&gt;Company logo&lt;/span&gt;&lt;/td&gt;
                    &lt;/tr&gt;
                &lt;/tbody&gt;
            &lt;/table&gt;
            &lt;/td&gt;
        &lt;/tr&gt;
        &lt;tr&gt;
			&lt;td style="text-align: left; padding: 8px"&gt;
			&lt;table style="width: 287px; height: 23px"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td style="text-align: left; color:black; font-weight:bold;"&gt;Invoice number&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 10px; padding: 8px"&gt;{%HTMLEncode(Order.OrderInvoiceNumber) |(user)administrator|(hash)630daa9f93f3d3a58ac0690aadfca6e772a23bad94415a4de9df32d3c55d2082%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
			&lt;td style="text-align: left; padding: 8px"&gt;
			&lt;table width="100%"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td style="text-align: left; color:black; font-weight:bold;"&gt;Order date:&lt;/td&gt;
						&lt;td style="text-align: right; padding: 8px"&gt;{% GetCurrentDateTimeString(Format(Order.OrderDate, "{0:g}"), (Customer.CustomerUser == null) ? "" : Customer.CustomerUser.UserName) |(user)administrator|(hash)2e24cb6f250eb5d5f3ee16aa80c2d84bc14a311ff74039ec9b287404fe8675f5%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td style="text-align: left; vertical-align: top; padding: 8px" width="50%"&gt;&lt;span style="color:black; font-weight:bold"&gt;Supplier&lt;/span&gt;&lt;br /&gt;
			&amp;nbsp;
			&lt;table&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td&gt;Company address&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
			&lt;td style="text-align: left; vertical-align: top; padding: 8px" width="50%"&gt;&lt;span style="color:black; font-weight:bold"&gt;Customer &lt;/span&gt;&lt;br /&gt;

			&lt;p&gt;{%BillingAddress.ApplyTransformation("Ecommerce.Transformations.Order_Address") |(user)administrator|(hash)2b199e22b9fbea388a9823f6f0724c0ce11ece68c1350abbbff79284caac7cdc%}&lt;/p&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td colspan="2"&gt;
			&lt;table width="100%"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td style="text-align: left; padding: 8px"&gt;&lt;span style="color:black; font-weight:bold"&gt;Payment option&lt;/span&gt;&lt;/td&gt;
						&lt;td style="text-align: left; padding: 8px"&gt;{%PaymentOption.PaymentOptionDisplayName |(user)administrator|(hash)3585ea97ef96fac2eabcbed0dd9f922eeb513d125b821089b590514cf0898610%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td colspan="2" style="text-align: left; padding: 8px"&gt;{%ContentTable.ApplyTransformation("Ecommerce.Transformations.Order_ContentTable", "Ecommerce.Transformations.Order_ContentTableHeader", "Ecommerce.Transformations.Order_ContentTableFooter") |(user)administrator|(hash)efe8c2e2cd0c86ca579c1ff41d58f56bc658abadd77d756bfb58ecd24db3991a%} 
			&lt;hr size="1" /&gt;
			&lt;table cellpadding="5" width="50%" style="float: right;"&gt;
				&lt;tbody&gt;
					&lt;tr&gt;
						&lt;td&gt;Subtotal&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 0px"&gt;{% HTMLEncode((TotalItemsPrice).Format(Currency.CurrencyFormatString)) |(user)administrator|(hash)49a45166e327501f63feea90cb83d4f9b47b2c4e49b7b1ca8c1f347f4221fe43%}&lt;/td&gt;
					&lt;/tr&gt;
					&lt;tr&gt;
						&lt;td colspan="2" style="padding: 0; text-align: right;"&gt;{% OrderRelatedDiscountSummaryItems.Any() ? OrderRelatedDiscountSummaryItems.ApplyTransformation("Ecommerce.Transformations.Order_OrderRelatedDiscountsContent", "Ecommerce.Transformations.Order_OrderRelatedDiscountsHeader", "Ecommerce.Transformations.Order_OrderRelatedDiscountsFooter") : "" |(user)administrator|(hash)525731e8690aaad70e0ff0a66e47571b134698c02fab4576587ab0a5ba34a8c0%}&lt;/td&gt;
					&lt;/tr&gt;
					&lt;tr&gt;
						&lt;td&gt;Shipping&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 0px"&gt;{% HTMLEncode(TotalShipping.Format(Currency.CurrencyFormatString)) |(user)administrator|(hash)4034500dddd77bba1b11dc80d211cd5ee7122a32984354d8ae997975f307e1c0%}&lt;/td&gt;
					&lt;/tr&gt;
					&lt;tr&gt;
						&lt;td&gt;&lt;span style="color:black; font-weight:bold"&gt;TOTAL (incl. tax):&lt;/span&gt;&lt;/td&gt;
						&lt;td style="text-align: right; padding-right: 0px; color:black; font-weight:bold"&gt;{% HTMLEncode(TotalPrice.Format(Currency.CurrencyFormatString))|(user)administrator|(hash)422b3d3e3dd96c7771b8999e32693bacbce28ca808e4732492299517c4add88b%}&lt;/td&gt;
					&lt;/tr&gt;
				&lt;/tbody&gt;
			&lt;/table&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
        &lt;tr&gt;
            &lt;td colspan="2"&gt;
            &lt;table width="100%"&gt;
                &lt;tbody&gt;
                    &lt;tr&gt;
                        &lt;td style="text-align: left;"&gt;&lt;span style="font-weight: bold;"&gt;Order note&lt;/span&gt;&lt;/td&gt;
                    &lt;/tr&gt;
                    &lt;tr&gt;
                        &lt;td style="text-align: left;"&gt;{%HTMLENcode(Order.OrderNote)|(user)administrator|(hash)bba87577df9e99aef3a361c931ed84d2b7dbfc9efc05349e0d7f52d4b9a0e401%}&lt;/td&gt;
                    &lt;/tr&gt;
                &lt;/tbody&gt;
            &lt;/table&gt;
            &lt;/td&gt;
        &lt;/tr&gt;
    &lt;/tbody&gt;
&lt;/table&gt;
&lt;/body&gt;
&lt;/html&gt;</EmailTemplateText>
      <EmailTemplateGUID>674d1b85-ce19-40bd-b2df-166a5891090a</EmailTemplateGUID>
      <EmailTemplateLastModified>2015-11-12T17:09:00.1598741+01:00</EmailTemplateLastModified>
      <EmailTemplateType>ecommerce</EmailTemplateType>
