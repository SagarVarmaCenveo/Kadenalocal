      <field allowempty="true" column="TradeMarketingManagerID" columntype="integer" guid="e76a0ab7-51db-43b3-934a-97db276e108e" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Trade Marketing Manager</fieldcaption>
        </properties>
        <settings>
          <controlname>DropDownListControl</controlname>
          <DisplayActualValueAsItem>False</DisplayActualValueAsItem>
          <EditText>False</EditText>
          <Query>
<![CDATA[
SELECT '-1' AS UserID,'(none)' AS UserName
UNION
SELECT 
UserID,
CASE WHEN ((FirstName+' '+LastName)=' ') THEN UserName ELSE (FirstName+' '+LastName) END
FROM 
CMS_User
WHERE
UserID IN 
(
SELECT UserID FROM CMS_UserRole WHERE RoleID=(SELECT TOP 1 RoleID FROM CMS_Role WHERE RoleName='KDA.TradeMarketingManager' AND SiteID={%CurrentSite.ID |(user)administrator|(hash)015efb55d982988e9c1a5fac114c96108d74def97e120cb2b399b08fa6ed69e9%}) AND UserID IN (Select UserID From CMS_UserSite Where SiteID={%CurrentSite.ID |(user)administrator|(hash)015efb55d982988e9c1a5fac114c96108d74def97e120cb2b399b08fa6ed69e9%})
)
AND
UserEnabled=1
]]>
          </Query>
          <SortItems>False</SortItems>
        </settings>
      </field>
