[ignore]
.*/node_modules/.*

[include]

[libs]
flow-typed

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
module.name_mapper='^app.dump\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/components/_dump/\1'
module.name_mapper='^app.smart\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/components/smart/\1'
module.name_mapper='^app.ac\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/AC/\1'
module.name_mapper='^app.globals\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/globals.js'
module.name_mapper='^app.consts\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/constants.js'
module.name_mapper='^app.reducers\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/reducers/\1'
module.name_mapper='^app.helpers\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/helpers/\1'
module.name_mapper='^app.ws\(\/?.*\)$' -> '<PROJECT_ROOT>/src/app/AC/_ws/\1'
