        "flow", "react", "es2015", "stage-0"
      ],
      "plugins": [
        "transform-react-remove-prop-types", "transform-object-rest-spread"
      ]
    },
    "development": {
      "presets": [
        "flow", "react", "es2015", "stage-0"
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    },
    "test": {
