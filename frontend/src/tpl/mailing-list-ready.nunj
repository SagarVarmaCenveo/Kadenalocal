              {{ svg('star--filled', 'template__icon--filled icon-star') }}
              {{ svg('star--unfilled', 'template__icon--unfilled icon-star') }}
            </div>
          </div>
          <div class="template__img" style="background-image: url(http://satyr.io/250-500x150-300?1)"></div>
