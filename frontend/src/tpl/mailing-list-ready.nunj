{% extends "layouts/default.nunj" %}
{% from "macro/svg.nunj" import svg %}

{% set title = 'List is ready' %}
{% set sidebarActive = 'K-List' %}
{% set breadcrumbs = [sidebarActive, 'New K-List'] %}
{% set headerTitle = 'List is now ready to be used' %}

