{% extends "layouts/default.nunj" %}

{% from "macro/sidebar.nunj" import sidebar %}
{% from "macro/breadcrumb.nunj" import breadcrumb %}
{% from "macro/select.nunj" import select %}
{% import "macro/svg.nunj" as svg %}

{% set title = "Mailing list" %}

{% block content %}
  {{ sidebar('Mailing list') }}
  <div class="content">
    <div class="content__inner">
      <div class="content-header">
        <div class="content-header__inner">
          <h1 class="content-header__page-name">Your processed list</h1>
        </div>
        {{ breadcrumb(['Home','Mailing list','New mailing list']) }}
      </div>
      <div class="content-block processed-list">
        <div class="processed-list__table-block">
          <div class="processed-list__table-heading processed-list__table-heading--success">
            <h3>332 addresses have been processed successfuly</h3>
          </div>
          <div class="processed-list__table-inner">
            <table class="table">
            <tbody>
                <tr>
                  <th>Name</th>
                  <th>Address Line 1</th>
                  <th>Address Line 2</th>
                  <th>City</th>
                  <th>State</th>
                  <th>Zip Code</th>
                </tr>
                <tr>
                  <td>Jane Doe</td>
