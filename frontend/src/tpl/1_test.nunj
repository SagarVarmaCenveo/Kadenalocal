{% extends "layouts/default.nunj" %}
{% from "macro/breadcrumb.nunj" import breadcrumb %}
{% from "macro/sidebar.nunj" import sidebar %}
{% import "macro/svg.nunj" as svg %}

{% set title = "Test" %}

{% block content %}

  {{ sidebar('Products') }}

  <div class="content">
    <div class="content__inner">
      <div class="content-header">
        <div class="content-header__inner">
          <h1 class="content-header__page-name">Test Header</h1>
        </div>
        {{ breadcrumb(['Home','Test']) }}
      </div>
      <div class="content-block">
        <div class="adress-card">
          <span>4001 Valley Industrial Blvd</span>
          <span>Shakopee</span>
          <span>MN 55379</span>
          <dib class="adress-card__btn-block">
            <a href="#" class="in-card-btn">
              {% call svg.template() %}
              {{svg.use(name='edit')}}
              {% endcall %}
              Edit
            </a>
            <a href="#" class="in-card-btn">
              {% call svg.template() %}
              {{svg.use(name='cross--dark')}}
              {% endcall %}
              Remove
            </a>
          </dib>
        </div>
      </div>
    </div>
  </div>

{% endblock %}
